# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
NEXT_PUBLIC_SUPABASE_STORAGE_BUCKET=files

# OpenAI Configuration (for text/code/PDF embeddings)
OPENAI_API_KEY=your-openai-api-key

# Google Cloud Vertex AI Configuration (for image/audio/video embeddings)
GOOGLE_CLOUD_PROJECT_ID=your-google-cloud-project-id
GOOGLE_CLOUD_LOCATION=us-central1
# Use absolute path or relative path from project root
# Example: ./keys.json or ./config/google-cloud-key.json
GOOGLE_APPLICATION_CREDENTIALS=./keys.json

# SerpAPI Configuration (for medical research via Google Scholar)
SERPAPI_API_KEY=your-serpapi-api-key

# Retell AI Configuration (for phone calls with patients)
RETELL_API_KEY=your-retell-api-key
RETELL_AGENT_ID=your-retell-agent-id
RETELL_FROM_NUMBER=+18445550123

# Website Scraping Configuration
# Enable Playwright for JavaScript rendering (requires: npm install playwright && npx playwright install chromium)
ENABLE_PLAYWRIGHT=true
# Use advanced scraper with smart detection (set to false to use basic fetch only)
USE_ADVANCED_SCRAPER=true

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

